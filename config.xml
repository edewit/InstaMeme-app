<?xml version='1.0' encoding='utf-8'?>
<widget id="org.feedhenry" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>InstaMeme</name>
    <description>
        Create a meme with your device.
    </description>
    <author email="feedhenry-dev@redhat.com" href="http://feedhenry.org">
        Feedhenry
    </author>
    <content src="index.html" />
    <access origin="*" />
    <engine name="android" />
    <engine name="ios" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.6.0" />
    <plugin name="cordova-plugin-camera" spec="~2.3.0" />
    <plugin name="aerogear-cordova-push" spec="~3.0.1" />
</widget>
